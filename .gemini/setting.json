{
  "mcpServers": {
    "chatkit": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "CONTEXT7_API_KEY": "YOUR_CHATKIT_API_KEY"
      }
    },
    "betterauth": {
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "YOUR_BETTERAUTH_API_KEY"
      }
    }
  }
}
